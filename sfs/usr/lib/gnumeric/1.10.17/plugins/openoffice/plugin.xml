<?xml version="1.0" encoding="UTF-8"?>
<plugin id="Gnumeric_OpenCalc">
	<information>
		<name>Open Document Format</name>
		<name xml:lang="cs">Formát Open Document</name>
		<name xml:lang="da">ODF</name>
		<name xml:lang="de">Open Document Format</name>
		<name xml:lang="el">Άνοιγμα μορφής εγγράφου</name>
		<name xml:lang="en_GB">Open Document Format</name>
		<name xml:lang="es">Formato Open Document</name>
		<name xml:lang="eu">Open Document Formatua</name>
		<name xml:lang="fi">Avoin asiakirjamuoto (ODF)</name>
		<name xml:lang="fr">Format Open Document</name>
		<name xml:lang="lt">Open Document Format</name>
		<name xml:lang="nb">Open Document-format</name>
		<name xml:lang="ru">Формат Open Document</name>
		<name xml:lang="sl">Zapis Open Document</name>
		<description>Open Document Format as used by OpenOffice, Lotus Symphony, etc.</description>
		<description xml:lang="cs">Formát Open Document používaný OpenOffice, Lotus Symphony, atd.</description>
		<description xml:lang="da">Open Document Format som brugt af OpenOffice, Lotus Symphony, et cetera.</description>
		<description xml:lang="de">Open Document Format, wie es u.a. von OpenOffice und Lotus Symphony benutzt wird</description>
		<description xml:lang="el">Άνοιγμα μορφής εγγράφου όπως χρησιμοποιείται από το OpenOffice, το Lotus Symphony, κλπ.</description>
		<description xml:lang="en_GB">Open Document Format as used by OpenOffice, Lotus Symphony, etc.</description>
		<description xml:lang="es">Formato Open Document como lo usan OpenOffice, Lotus Symphony, etc.</description>
		<description xml:lang="eu">Open Document formatua OpenOffice.org, Lotus Symphony eta beste batzuk erabilia.</description>
		<description xml:lang="fi">Avoin asiakirjamuoto (ODF), jota käyttävät OpenOffice, Lotus Symphony jne.</description>
		<description xml:lang="fr">Format Open Document tel qu'utilisé par OpenOffice, Lotus Symphony, etc.</description>
		<description xml:lang="lt">Open Document Format, naudojamas OpenOffice.org, Lotus Symphony ir kt.</description>
		<description xml:lang="nb">Open Document-format som brukes av OpenOffice, Lotus Symphony, etc.</description>
		<description xml:lang="ru">Формат Open Document используемый OpenOffice, Lotus Symphony и т.п.</description>
	</information>
	<loader type="Gnumeric_Builtin:module">
		<attribute value="openoffice" name="module_file"/>
	</loader>
	<services>
		<service type="file_opener" probe="TRUE" priority="1" id="openoffice">
			<information>
				<description>Open Document Format (*.sxc, *.ods)</description>
				<description xml:lang="cs">Formát Open Document (*.sxc, *.ods)</description>
				<description xml:lang="da">Open Document Format (*.sxc, *.ods)</description>
				<description xml:lang="de">Open Document Format (*.sxc, *.ods)</description>
				<description xml:lang="el">Άνοιγμα μορφής εγγράφου (*.sxc, *.ods)</description>
				<description xml:lang="en_GB">Open Document Format (*.sxc, *.ods)</description>
				<description xml:lang="es">Formato Open Document (*.sxc, *.ods)</description>
				<description xml:lang="eu">Open Document formatua (*.sxc, *.ods)</description>
				<description xml:lang="fi">Avoin ODF-asiakirjamuoto (*.sxc, *.ods)</description>
				<description xml:lang="fr">Format Open Document (*.sxc, *.ods)</description>
				<description xml:lang="lt">Open Document Format (*.sxc, *.ods)</description>
				<description xml:lang="nb">Open Document-format (*.sxc, *.ods)</description>
				<description xml:lang="ru">Формат Open Document (*.sxc, *.ods)</description>
				<description xml:lang="sl">Open Document (*.sxc, *.ods)</description>
			</information>
			<suffixes>
				<suffix>ods</suffix>
				<suffix>ots</suffix>
				<suffix>sxc</suffix>
				<suffix>stc</suffix>
			</suffixes>
			<mime-types>
				<mime-type>vnd.oasis.opendocument.spreadsheet</mime-type>
				<mime-type>vnd.oasis.opendocument.spreadsheet-template</mime-type>
				<mime-type>application/vnd.sun.xml.calc</mime-type>
				<mime-type>application/vnd.sun.xml.calc.template</mime-type>
			</mime-types>
		</service>
		<service type="file_saver" overwrite_files="TRUE" id="openoffice" format_level="auto" file_extension="ods">
			<information>
				<description>ODF/OpenOffice without foreign elements (*.ods)</description>
				<description xml:lang="cs">ODF/OpenOffice bez cizích prvků (*.ods)</description>
				<description xml:lang="da">ODF/OpenOffice uden fremmedelementer (*.ods)</description>
				<description xml:lang="de">ODF/OpenOffice ohne Fremdelemente (*.ods)</description>
				<description xml:lang="el">ODF/OpenOfficeχωρίς ξένα στοιχεία (*.ods)</description>
				<description xml:lang="en_GB">ODF/OpenOffice without foreign elements (*.ods)</description>
				<description xml:lang="es">ODF/OppenOffice sin elementos externos (*.ods)</description>
				<description xml:lang="eu">ODF/OpenOffice kanpoko elementurik gabe (*.ods)</description>
				<description xml:lang="fi">ODF/OpenOffice ilman lisätekijöitä (*.ods)</description>
				<description xml:lang="fr">ODF/OpenOffice sans éléments externes (*.ods)</description>
				<description xml:lang="lt">ODF/OpenOffice.org be pašalinių elementų (*.ods)</description>
				<description xml:lang="ru">ODF/OpenOffice без дополнительных элементов (*.ods)</description>
				<description xml:lang="sl">ODF/OpenOffice brez tujih predmetov (*.ods)</description>
			</information>
		</service>
		<service type="file_saver" overwrite_files="TRUE" id="odf" format_level="auto" file_extension="ods">
			<information>
				<description>ODF/OpenOffice with foreign elements (*.ods)</description>
				<description xml:lang="cs">ODF/OpenOffice s cizími prvky (*.ods)</description>
				<description xml:lang="da">ODF/OpenOffice med fremmedelementer (*.ods)</description>
				<description xml:lang="de">ODF/OpenOffice mit Fremdelementen (*.ods)</description>
				<description xml:lang="el">ODF/OpenOffice με ξένα στοιχεία (*.ods)</description>
				<description xml:lang="en_GB">ODF/OpenOffice with foreign elements (*.ods)</description>
				<description xml:lang="es">ODF/OppenOffice con elementos externos (*.ods)</description>
				<description xml:lang="eu">ODF/OpenOffice kanpoko elementuekin (*.ods)</description>
				<description xml:lang="fi">ODF/OpenOffice lisätekijöin (*.ods)</description>
				<description xml:lang="fr">ODF/OpenOffice avec éléments externes (*.ods)</description>
				<description xml:lang="lt">ODF/OpenOffice.org su pašaliniais elementais (*.ods)</description>
				<description xml:lang="ru">ODF/OpenOffice с дополнительными элементами (*.ods)</description>
				<description xml:lang="sl">ODF/OpenOffice s tujimi predmeti (*.ods)</description>
			</information>
		</service>
	</services>
</plugin>